{
  "$schema": "https://turborepo.com/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "inputs": [
        "$TURBO_DEFAULT$",
        ".env*"
      ],
      "outputs": [
        "dist/**",
        "!dist/**/*.map"
      ]
    },
    "db:migrate": {
      "cache": false,
      "outputs": []
    },
    "dev": {
      "cache": false,
      "persistent": true,
      "dependsOn": [
        "db:migrate"
      ],
      "outputs": []
    }
  }
}